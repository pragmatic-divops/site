{
  "name": "@pragmatic-divops/site",
  "description": "the pragmatic-divops website",
  "license": "MIT",
  "private": true,
  "repository": "pragmatic-divops/site",
  "bugs": "https://github.com/pragmatic-divops/site/issues",
  "homepage": "https://pragmatic-divos.dev",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "predeploy:netlify": "run-s build",
    "deploy:netlify": "npx --package netlify-cli netlify deploy --dir=./lib/",
    "lint:lockfile": "lockfile-lint",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "run-s lint:js -- --fix",
    "lint:md": "remark . --frail",
    "generate:md": "remark . --output",
    "lint:sensitive": "ban",
    "clean": "gatsby clean",
    "start": "run-s dev",
    "prebuild": "run-s clean",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "test:served:vulnerable": "is-website-vulnerable http://localhost:8000",
    "test:served": "start-server-and-test 'npm start' http://localhost:8000 'npm-run-all --print-label --parallel test:served:*'",
    "test:served:smoke": "run-s cypress:run",
    "dev": "gatsby develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "disabled:lint:peer": "npm ls >/dev/null"
  },
  "dependencies": {
    "gatsby": "2.24.85",
    "react": "16.14.0",
    "react-dom": "16.14.0"
  },
  "devDependencies": {
    "@babel/register": "7.12.1",
    "@form8ion/babel-preset": "1.6.34",
    "@pragmatic-divops/commitlint-config": "1.0.2",
    "@pragmatic-divops/eslint-config": "1.0.7",
    "@pragmatic-divops/eslint-config-cypress": "1.0.0",
    "@pragmatic-divops/eslint-config-react": "1.0.0",
    "@pragmatic-divops/remark-preset": "1.0.2",
    "@travi/any": "2.0.13",
    "babel-plugin-istanbul": "6.0.0",
    "ban-sensitive-files": "1.9.12",
    "cypress": "5.4.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "4.3.0",
    "is-website-vulnerable": "1.14.5",
    "lockfile-lint": "4.3.7",
    "npm-run-all": "4.1.5",
    "remark-cli": "9.0.0",
    "remark-toc": "7.0.0",
    "rimraf": "3.0.2",
    "start-server-and-test": "1.11.5"
  }
}
